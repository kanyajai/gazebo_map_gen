<launch>
    <include file="$(find gazebo_ros)/launch/empty_world.launch" />
    <param name="map_description" command="rosrun xacro xacro $(find gazebo_map_gen)/urdf/office.urdf" />
    <node name="spawn_map" pkg="gazebo_ros" type="spawn_model" args="-urdf -param map_description -model map" />
    <param name="robot_description" command="$(find xacro)/xacro --inorder $(find turtlebot3_description)/urdf/turtlebot3_waffle.urdf.xacro" />
    <node pkg="gazebo_ros" type="spawn_model" name="spawn_urdf" args="-urdf -model turtlebot3_waffle -x 1 -y 1 -param robot_description" /> 
</launch>
